                    <div class="flex-shrink-0">
                        <p class="fs-14 fw-semibold mb-1">@Model.DataDasar.NamaWP</p>
                    </div>
                </div>
                <div class="d-flex">
                    <p class="fs-14 text-muted flex-grow-1">Alamat</p>
                    <div class="flex-shrink-0">
                        <p class="fs-14 fw-semibold mb-1">@Model.DataDasar.Alamat</p>
                    </div>
                </div>
                <div class="d-flex">
                    <p class="fs-14 text-muted flex-grow-1">Total Kapasitas</p>
                    <div class="flex-shrink-0">
                        <p class="fs-14 fw-semibold mb-1">@Model.DataDasar.TotalKapasitas Kendaraan</p>
                    </div>
                </div>
            </div>
        </div>

        <div class="card">
            <div class="card-header d-flex bg-soft-secondary">
                <div class="flex-grow-1">
                    <h4 class="card-title fs-16 mb-0">
                        Potensi Pajak
                    </h4>
                </div>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="card border border-secondary border-2">
                            <div class="card-body bg-secondary bg-opacity-25">
                                <h4 class="card-title fs-14 mb-4">
                                    Potensi Pajak per Bulan
                                </h4>
                                <h4 class="card-title fs-18 mb-0">
                                    @Model.DataPotensi.PotensiBulanan.ToString("C")
                                </h4>
                                <p class="fs-12">10% dari omzet bulanan</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class="card border border-secondary border-2">
                            <div class="card-body bg-secondary bg-opacity-25">
                                <h4 class="card-title fs-14 mb-4">
                                    Potensi Pajak per Tahun
                                </h4>
                                <h4 class="card-title fs-18 mb-0">
                                    @Model.DataPotensi.PotensiTahunan.ToString("C")
                                </h4>
                                <p class="fs-12">Proyeksi untuk 12 bulan</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="card">
