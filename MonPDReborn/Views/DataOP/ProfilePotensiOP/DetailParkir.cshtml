                         aria-valuenow="@Model.DataKapasitas.PersenMotorHariKerja.ToString("F0")"
                         aria-valuemin="0" aria-valuemax="100"
                         style="width: @Model.DataKapasitas.PersenMotorHariKerja.ToString("F0")%">
