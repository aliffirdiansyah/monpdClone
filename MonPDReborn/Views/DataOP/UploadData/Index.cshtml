@section Scripts{
    <script>
        function UploadHotel() {
            var formData = new FormData($('#UploadHotel')[0]);

            $.ajax({
                url: '@Url.Action("UploadPotensiHotel", "UploadData")',
                type: 'POST',
                data: formData,
                contentType: false,
                processData: false,
                beforeSend: function () {
                    $("#loader").show();
                },
                complete: function () {
                    $("#loader").hide();
                },
                success: function (response) {
                    if (response.Status === 1) {
                        Swal.fire({
                            icon: 'success',
                            title: 'Berhasil',
                            text: response.Message
                        });

                        $('#UploadHotel')[0].reset();
                    } else {
                        Swal.fire({
                            icon: 'error',
                            title: 'Gagal',
                            text: response.Message
                        });
                    }
                },
                error: function () {
                    Swal.fire({
                        icon: 'error',
                        title: 'Error',
                        text: 'Terjadi kesalahan saat mengirim data.'
                    });
                }
            });
        }

