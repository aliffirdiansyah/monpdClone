                            <i class="ri-swap-line " style="font-size: 20px; line-height: 1; color:#374151"></i>
                        </div>
                        <h6 class="mb-0 fs-16 fw-bold">
                            @(EnumFactory.EPajak.OpsenBbnkb.GetDescription())
                        </h6>
                    </div>
                    <h6 class="mb-0 fs-16 text-primary fw-bold">@Model.Data.PersentaseOpsenBbnkb%</h6>
                </div>


                <div class="progress mb-3" style="height: 10px;">
                    <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" style="background-color:#374151; width: @Model.Data.PersentaseOpsenBbnkb.ToString("N0")%;" aria-valuenow="@Model.Data.PersentaseOpsenBbnkb.ToString("N0")" aria-valuemin="0" aria-valuemax="100"></div>
                </div>

                <div class="row text-muted mb-2" style="cursor:pointer;" onclick="fnMonitoringBulanan('@((int)EnumFactory.EPajak.OpsenBbnkb)')">
                    <div class="col">
                        <small>Realisasi</small><br>
                        <span class="fw-bold fs-15 text-dark">Rp @Model.Data.RealisasiOpsenBbnkb.ToString("n0")</span>
                    </div>
                    <div class="col text-end">
                        <small>Target</small><br>
                        <span class="fw-bold fs-15">Rp @Model.Data.TargetOpsenBbnkb.ToString("n0")</span>
                    </div>
                </div>

                <div class="border-top pt-2 d-flex justify-content-between align-items-center">
                    <span class="text-muted">Total Objek Pajak</span>
                    <a href="@Url.Action("Index", "ProfileOP")">
                        <span class="fw-bold">@Model.Data.JumlahOpOpsenBbnkb.ToString("n0") OP</span>
                    </a>
                </div>
            </div>
        </div>
    </div>

    <div class="col-lg-12 col-sm-6">
        <div class="card card-animate h-100 shadow-lg rounded-3" style="border-top: 6px solid #39FF14; height:87%;">
            <div class="card-header">
                <div class="d-flex justify-content-between align-items-center mb-3">
                    <div class="d-flex align-items-start flex-column">
                        <div class="d-flex align-items-center mb-1">
                            <div class="rounded-circle d-flex justify-content-center align-items-center me-2"
                                 style="width: 35px; height: 35px; aspect-ratio: 1 / 1; flex-shrink: 0; background-color: rgba(175, 212, 210, 0.2);">
                                <i class="ri-funds-line" style="font-size: 20px; line-height: 1; color: #39FF14;"></i>
                            </div>
                            <h6 class="mb-0 fs-16 fw-bold">
                                TOTAL SEMUA PAJAK
                            </h6>
                        </div>
                        <div class="ms-5">
                            <span class="fs-14 text-muted">
                                <i class="bx bx-calendar me-1"></i>
                                @DateTime.Now.ToString("dd MMM yyyy", new System.Globalization.CultureInfo("id-ID"))
                            </span>
                        </div>
                    </div>
                    <div class="border-top pt-2 d-flex justify-content-between align-items-center">
                        <button onclick="fnMonitoringBulanan('@((int)EnumFactory.EPajak.Semua)')" class="btn btn-sm btn-outline-info">
                            <i class="ri-file-search-line"></i> Detail
                        </button>
                    </div>
                </div>
            </div>
            <div class="card-body">

                <div class="row text-muted mb-2">
                    <div class="col">
                        <span class="fs-16">Realisasi</span><br>
                        <span class="fw-bold fs-18 text-dark">Rp @Model.Data.TotalRealisasi.ToString("n0")</span>
                    </div>
                    <div class="col text-end">
                        <span class="fs-16">Terget</span><br>
                        <span class="fw-bold fs-18">Rp @Model.Data.TotalTarget.ToString("n0")</span>
                    </div>
                </div>
                <hr />
                <!-- ApexCharts radial -->
                <div class="row">
                    <div class="col-lg-6 col-md-12 col-sm-12">
                        <div id="gradient_radialbar" data-colors='["--vz-success"]' class="apex-charts" dir="ltr"></div>
                    </div>
                    <div class="col-lg-6 col-md-12 col-sm-12">
                        <div id="simple_pie_chart" data-colors='["#3B82F6", "#00712D", "#FF7A30", "#FACC15", "#8B5CF6", "#EF4444", "#EC4899", "#06B6D4", "#FF00FF", "#FFB347", "#374151"]' class="apex-charts" dir="ltr"></div>
                        @* <div id="simple_dount_chart" data-colors='["#3B82F6", "#00712D", "#FF7A30", "#FACC15", "#8B5CF6", "#EF4444", "#EC4899", "#06B6D4", "#FF00FF", "#FFB347", "#374151"]' class="apex-charts w-100" dir="ltr"></div> *@
                    </div>
                </div>

            </div>
        </div>
    </div>

</div>


<script>
    //  Line chart datalabel
    var linechartDatalabelColors = getChartColorsArray("line_chart_datalabel");
    var options = {
        chart: {
            height: 500,
            type: 'line',
            zoom: {
                enabled: false
            },
            toolbar: {
                show: false
            }
        },
        colors: linechartDatalabelColors,
        dataLabels: {
            enabled: false,
        },
        stroke: {
            width: [3, 3],
            curve: 'straight'
        },
        series: [{
            name: "Target",
            data: [@(Model.ChartData.Target1), @(Model.ChartData.Target2), @(Model.ChartData.Target3), @(Model.ChartData.Target4), @(Model.ChartData.Target5), @(Model.ChartData.Target6), @(Model.ChartData.Target7), @(Model.ChartData.Target8), @(Model.ChartData.Target9), @(Model.ChartData.Target10), @(Model.ChartData.Target11), @(Model.ChartData.Target12)]
        }, {
            name: "Realisasi",
            data: [@(Model.ChartData.Realisasi1), @(Model.ChartData.Realisasi2), @(Model.ChartData.Realisasi3), @(Model.ChartData.Realisasi4), @(Model.ChartData.Realisasi5), @(Model.ChartData.Realisasi6), @(Model.ChartData.Realisasi7), @(Model.ChartData.Realisasi8), @(Model.ChartData.Realisasi9), @(Model.ChartData.Realisasi10), @(Model.ChartData.Realisasi11), @(Model.ChartData.Realisasi12)]
        }],
        title: {
            text: 'Target & Realisasi PAD 2025',
            align: 'left',
            style: {
                fontWeight: 500,
            },
        },
        grid: {
            row: {
                colors: ['transparent', 'transparent'], // takes an array which will be repeated on columns
                opacity: 0.2
            },
            borderColor: '#f1f1f1'
        },
        markers: {
            style: 'inverted',
            size: 6
        },
        xaxis: {
            categories: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Des'],
            title: {
                text: 'Bulan'
            }
        },
        yaxis: {
            title: {
                text: 'Nominal'
            },
            labels: {
                formatter: function (val) {
                    return 'Rp' + val.toLocaleString('id-ID');
                }
            }
        },
        tooltip: {
            y: {
                formatter: function (val) {
                    return 'Rp' + val.toLocaleString('id-ID');
                }
            }
        },
        legend: {
            position: 'top',
            horizontalAlign: 'right',
            floating: true,
            offsetY: -25,
            offsetX: -5
        },
        responsive: [{
            breakpoint: 600,
            options: {
                chart: {
                    toolbar: {
                        show: false
                    }
                },
                legend: {
                    show: false
                },
            }
        }]
    }

    var chart = new ApexCharts(document.querySelector("#line_chart_datalabel"), options);

    chart.render();

    var chartRadialbarGradientColors = getChartColorsArray("gradient_radialbar");
    if (chartRadialbarGradientColors) {
        var options = {
            series: [@Model.Data.TotalPersentase.ToString("n2").Replace(",", ".")],
            chart: {
                height: 400,
                type: "radialBar",
                toolbar: { show: false }
            },
            plotOptions: {
                radialBar: {
                    startAngle: -135,
                    endAngle: 225,
                    hollow: {
                        margin: 0,
                        size: "70%"
                    },
                    track: {
                        strokeWidth: "67%",
                        margin: 0
                    },
                    dataLabels: {
                        show: true,
                        name: {
                            show: true,
                            fontSize: "24px",
                            fontWeight: 600,
                            offsetY: 40, // jarak dari tengah
                            color: "#666"
                        },
                        value: {
                            offsetY: -10, // geser ke atas dari tengah
                            formatter: function (val) {
                                return parseFloat(val).toFixed(2) + " %";
                            },
                            color: "#111",
                            fontSize: "28px",
                            fontWeight: 700,
                            show: true
                        }
                    }
                }
            },
            fill: {
                type: "gradient",
                gradient: {
                    shade: "dark",
                    type: "horizontal",
                    shadeIntensity: 0.5,
                    gradientToColors: chartRadialbarGradientColors,
                    inverseColors: true,
                    opacityFrom: 1,
                    opacityTo: 1,
                    stops: [0, 100]
                }
            },
            stroke: {
                lineCap: "round"
            },
            responsive: [
            {
            breakpoint: 1200, // jika layar < 992px
            options: {
                chart: {
                    height: 300
                },
                legend: {
                    position: "bottom", // pindahkan legend ke bawah
                    offsetX: 0,
                    offsetY: 10,
                    fontSize: "25px",
                    itemMargin: {
                        horizontal: 5,
                        vertical: 2
                    }
                }
            }
        }
        ],
            labels: ["Total Persentase"]
        };

        var chart = new ApexCharts(document.querySelector("#gradient_radialbar"), options);
        chart.render();
    }

    var chartPieBasicColors = getChartColorsArray("simple_pie_chart");
    if (chartPieBasicColors) {
        var seriesData = [
            @Model.Data.PersentaseHotel.ToString("n2").Replace(",", "."),
            @Model.Data.PersentaseMamin.ToString("n2").Replace(",", "."),
            @Model.Data.PersentaseHiburan.ToString("n2").Replace(",", "."),
            @Model.Data.PersentaseListrik.ToString("n2").Replace(",", "."),
            @Model.Data.PersentaseParkir.ToString("n2").Replace(",", "."),
            @Model.Data.PersentaseReklame.ToString("n2").Replace(",", "."),
            @Model.Data.PersentaseAbt.ToString("n2").Replace(",", "."),
            @Model.Data.PersentasePbb.ToString("n2").Replace(",", "."),
            @Model.Data.PersentaseBphtb.ToString("n2").Replace(",", "."),
            @Model.Data.PersentaseOpsenPkb.ToString("n2").Replace(",", "."),
            @Model.Data.PersentaseOpsenBbnkb.ToString("n2").Replace(",", ".")
        ];

        var labelData = [
            "@(EnumFactory.EPajak.JasaPerhotelan.GetDescription())",
            "@(EnumFactory.EPajak.MakananMinuman.GetDescription())",
            "@(EnumFactory.EPajak.JasaKesenianHiburan.GetDescription())",
            "@(EnumFactory.EPajak.TenagaListrik.GetDescription())",
            "@(EnumFactory.EPajak.JasaParkir.GetDescription())",
            "@(EnumFactory.EPajak.Reklame.GetDescription())",
            "@(EnumFactory.EPajak.AirTanah.GetDescription())",
            "@(EnumFactory.EPajak.PBB.GetDescription())",
            "@(EnumFactory.EPajak.BPHTB.GetDescription())",
            "@(EnumFactory.EPajak.OpsenPkb.GetDescription())",
            "@(EnumFactory.EPajak.OpsenBbnkb.GetDescription())"
        ];

        var total = seriesData.reduce((a, b) => a + parseFloat(b), 0);

        var options = {
            series: seriesData,
            chart: {
                height: 500,
                type: "pie"
            },
            labels: labelData,
            legend: {
                position: "right",
                offsetY: 50,
                offsetX: -20,
                fontWeight: 600,
                fontSize: "13px",
                formatter: function (seriesName, opts) {
                    var value = opts.w.globals.series[opts.seriesIndex];
                    var percent = total > 0 ? ((value / total) * 100).toFixed(2) : 0;
                    return seriesName + ": " + percent + " %";
                },
                labels: {
                    useSeriesColors: false
                },
                itemMargin: {
                    horizontal: 0,
                    vertical: 2
                }
            },
            dataLabels: {
                enabled: true,
                formatter: function (val, opts) {
                    var value = opts.w.globals.series[opts.seriesIndex];
                    return value.toFixed(2) + " %";
                },
                style: {
                    fontWeight: "bold"
                },
                dropShadow: {
                    enabled: false
                }
            },
            responsive: [
                {
                    breakpoint: 1200,
                    options: {
                        chart: {
                            height: 500
                        },
                        legend: {
                            position: "bottom",
                            offsetX: 0,
                            offsetY: 10,
                            fontSize: "5px",
                            itemMargin: {
                                horizontal: 5,
                                vertical: 2
                            }
                        }
                    }
                }
            ],
            colors: chartPieBasicColors.slice(0, 11)
        };

        var chart = new ApexCharts(document.querySelector("#simple_pie_chart"), options);
        chart.render();
    }
</script>