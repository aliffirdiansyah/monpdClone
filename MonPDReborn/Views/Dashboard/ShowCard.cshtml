            <div class="card-body">
                <div class="d-flex justify-content-between align-items-center mb-3" style="cursor:pointer;" onclick="fnMonitoringBulanan('@((int)EnumFactory.EPajak.JasaPerhotelan)')">
                    <div class="d-flex align-items-center">
                        <div class="bg-soft-info rounded-circle d-flex justify-content-center align-items-center me-2" style=" width: 35px; height: 35px; aspect-ratio: 1 / 1; flex-shrink: 0;">
                            <i class=" ri-hotel-line  text-info" style="font-size: 20px; line-height: 1;"></i>
                        </div>
                        <h6 class="mb-0 fs-16 fw-bold">
                            @(EnumFactory.EPajak.JasaPerhotelan.GetDescription())
                        </h6>
                    </div>
                    <h6 class="mb-0 fs-16 text-primary fw-bold">@Model.Data.PersentaseHotel%</h6>
                </div>


                <div class="progress mb-3" style="height: 10px;">
                    <div class="progress-bar progress-bar-striped progress-bar-animated bg-info" role="progressbar" style="width: @Model.Data.PersentaseHotel.ToString("N0")%;" aria-valuenow="@Model.Data.PersentaseHotel.ToString("N0")" aria-valuemin="0" aria-valuemax="100"></div>
                </div>

                <div class="row text-muted mb-2" style="cursor:pointer;" onclick="fnMonitoringBulanan('@((int)EnumFactory.EPajak.JasaPerhotelan)')">
                    <div class="col">
                        <small>Realisasi</small><br>
                        <span class="fw-bold fs-15 text-dark">Rp @Model.Data.RealisasiHotel.ToString("n0")</span>
                    </div>
                    <div class="col text-end">
                        <small>Target</small><br>
                        <span class="fw-bold fs-15">Rp @Model.Data.TargetHotel.ToString("n0")</span>
                    </div>
                </div>

                <div class="border-top pt-2 d-flex justify-content-between align-items-center mb-2">
                    <span class="text-muted">Total Objek Pajak</span>
                    <a href="@Url.Action("Index", "ProfileOP")">
                        <span class="fw-bold">@Model.Data.JumlahOpHotel.ToString("n0") OP</span>
                    </a>
                </div>

            </div>
        </div>
    </div>

