      <Query Type="CustomSqlQuery" Name="SeriesPBB">
        <Sql>SELECT A.TAHUN_BUKU,TARGET,SPPT_MURNI,KETETAPAN_MURNI,SPPT_TUNGGAKAN,KETETAPAN_TUNGGAKAN,REALISASI_MURNI,REALISASI_TUNGGAKAN,REALISASI_SPPT_MURNI,REALISASI_SPPT_TUNGGAKAN,
            (REALISASI_MURNI / NULLIF(TARGET, 0)) AS RASIO_MURNI,
            (REALISASI_TUNGGAKAN / NULLIF(TARGET, 0)) AS RASIO_TUNGGAKAN,
            ( (NVL(REALISASI_MURNI,0)+NVL(REALISASI_TUNGGAKAN,0)) / NULLIF(TARGET, 0)) AS PENCAPAIAN
FROM (
SELECT TAHUN_BUKU,NAMA,SUM(TARGET) TARGET 
FROM VW_TARGET_BULAN_6
WHERE PAJAK_ID=9
GROUP BY TAHUN_BUKU,NAMA
) A
LEFT JOIN (
SELECT TAHUN_BUKU,
            SUM(DECODE(JENIS_BUKU,0,1,0)) SPPT_MURNI,
            SUM(DECODE(JENIS_BUKU,0,KETETAPAN,0)) KETETAPAN_MURNI,
            SUM(DECODE(JENIS_BUKU,1,1,0)) SPPT_TUNGGAKAN,
            SUM(DECODE(JENIS_BUKU,1,KETETAPAN,0)) KETETAPAN_TUNGGAKAN,
            SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR IS NOT NULL AND TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=0 THEN REALISASI
                    ELSE
                    0 END
                 ) REALISASI_MURNI,
            SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR IS NOT NULL AND TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=1 THEN REALISASI
                    ELSE
                    0 END
                 ) REALISASI_TUNGGAKAN,     
              SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR IS NOT NULL AND TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=0 THEN 1
                    ELSE
                    0 END
                 ) REALISASI_SPPT_MURNI,
            SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR IS NOT NULL AND TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=1 THEN 1
                    ELSE
                    0 END
                 ) REALISASI_SPPT_TUNGGAKAN        
FROM MV_DBE_MON_PBB
GROUP BY TAHUN_BUKU
) B ON A.TAHUN_BUKU=B.TAHUN_BUKU
ORDER BY TAHUN_BUKU DESC</Sql>
      </Query>
      <Query Type="CustomSqlQuery" Name="PivotSeries">
        <Sql>SELECT tahun_buku,
    target,
    REGEXP_SUBSTR(jenis_subjenis, '[^:]+', 1, 1) AS subjenis,
    REGEXP_SUBSTR(jenis_subjenis, '[^:]+', 1, 2) AS jenis,
    nilai
FROM (
SELECT A.TAHUN_BUKU,TARGET,SPPT_MURNI,KETETAPAN_MURNI,SPPT_TUNGGAKAN,KETETAPAN_TUNGGAKAN,REALISASI_MURNI,REALISASI_TUNGGAKAN,REALISASI_SPPT_MURNI,REALISASI_SPPT_TUNGGAKAN
FROM (
SELECT TAHUN_BUKU,NAMA,SUM(TARGET) TARGET 
FROM VW_TARGET_BULAN_6
WHERE PAJAK_ID=9
GROUP BY TAHUN_BUKU,NAMA
) A
LEFT JOIN (
SELECT TAHUN_BUKU,
            SUM(DECODE(JENIS_BUKU,0,1,0)) SPPT_MURNI,
            SUM(DECODE(JENIS_BUKU,0,KETETAPAN,0)) KETETAPAN_MURNI,
            SUM(DECODE(JENIS_BUKU,1,1,0)) SPPT_TUNGGAKAN,
            SUM(DECODE(JENIS_BUKU,1,KETETAPAN,0)) KETETAPAN_TUNGGAKAN,
            SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR IS NOT NULL AND TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=0 THEN REALISASI
                    ELSE
                    0 END
                 ) REALISASI_MURNI,
            SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR IS NOT NULL AND TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=1 THEN REALISASI
                    ELSE
                    0 END
                 ) REALISASI_TUNGGAKAN,     
              SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR IS NOT NULL AND TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=0 THEN 1
                    ELSE
                    0 END
                 ) REALISASI_SPPT_MURNI,
            SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR IS NOT NULL AND TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=1 THEN 1
                    ELSE
                    0 END
                 ) REALISASI_SPPT_TUNGGAKAN        
FROM MV_DBE_MON_PBB
GROUP BY TAHUN_BUKU
) B ON A.TAHUN_BUKU=B.TAHUN_BUKU
ORDER BY TAHUN_BUKU DESC
) A
UNPIVOT (
    nilai FOR jenis_subjenis IN (
        sppt_murni        AS 'SPPT:Murni',
        ketetapan_murni   AS 'Ketetapan:Murni',
        sppt_tunggakan    AS 'SPPT:Tunggakan',
        ketetapan_tunggakan AS 'Ketetapan:Tunggakan'
    )
)</Sql>
      </Query>
      <Query Type="CustomSqlQuery" Name="DetailBulan">
        <Sql>SELECT A.TAHUN_BUKU,BULAN,TARGET,SPPT_MURNI,KETETAPAN_MURNI,SPPT_TUNGGAKAN,KETETAPAN_TUNGGAKAN,REALISASI_MURNI,REALISASI_TUNGGAKAN,REALISASI_SPPT_MURNI,REALISASI_SPPT_TUNGGAKAN,
            (REALISASI_MURNI / NULLIF(TARGET, 0)) AS RASIO_MURNI,
            (REALISASI_TUNGGAKAN / NULLIF(TARGET, 0)) AS RASIO_TUNGGAKAN,
            ( (NVL(REALISASI_MURNI,0)+NVL(REALISASI_TUNGGAKAN,0)) / NULLIF(TARGET, 0)) AS PENCAPAIAN
FROM (
SELECT TAHUN_BUKU,BULAN,SUM(TARGET) TARGET 
FROM VW_TARGET_BULAN_6
WHERE PAJAK_ID=9
GROUP BY TAHUN_BUKU,BULAN
) A
LEFT JOIN (
SELECT TAHUN_BUKU,EXTRACT(MONTH FROM TGL_BAYAR) BULAN_BAYAR,
            SUM(DECODE(JENIS_BUKU,0,1,0)) SPPT_MURNI,
            SUM(DECODE(JENIS_BUKU,0,KETETAPAN,0)) KETETAPAN_MURNI,
            SUM(DECODE(JENIS_BUKU,1,1,0)) SPPT_TUNGGAKAN,
            SUM(DECODE(JENIS_BUKU,1,KETETAPAN,0)) KETETAPAN_TUNGGAKAN,
            SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR IS NOT NULL AND TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=0 THEN REALISASI
                    ELSE
                    0 END
                 ) REALISASI_MURNI,
            SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR IS NOT NULL AND TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=1 THEN REALISASI
                    ELSE
                    0 END
                 ) REALISASI_TUNGGAKAN,     
              SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR IS NOT NULL AND TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=0 THEN 1
                    ELSE
                    0 END
                 ) REALISASI_SPPT_MURNI,
            SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR IS NOT NULL AND TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=1 THEN 1
                    ELSE
                    0 END
                 ) REALISASI_SPPT_TUNGGAKAN        
FROM MV_DBE_MON_PBB
GROUP BY TAHUN_BUKU,EXTRACT(MONTH FROM TGL_BAYAR)
) B ON A.TAHUN_BUKU=B.TAHUN_BUKU AND A.BULAN=B.BULAN_BAYAR
ORDER BY BULAN_BAYAR ASC</Sql>
      </Query>
      <Query Type="CustomSqlQuery" Name="AllinOne">
        <Sql>SELECT A.TAHUN_BUKU,A.BULAN,A.UPTB,A.TARGET,B.SPPT_MURNI,B.KETETAPAN_MURNI,B.SPPT_TUNGGAKAN,B.KETETAPAN_TUNGGAKAN,
            REALISASI_MURNI,REALISASI_TUNGGAKAN,REALISASI_SPPT_MURNI,REALISASI_SPPT_TUNGGAKAN,
            (REALISASI_MURNI / NULLIF(TARGET, 0)) AS RASIO_MURNI,
            (REALISASI_TUNGGAKAN / NULLIF(TARGET, 0)) AS RASIO_TUNGGAKAN,
            ( (NVL(REALISASI_MURNI,0)+NVL(REALISASI_TUNGGAKAN,0)) / NULLIF(TARGET, 0)) AS PENCAPAIAN
FROM(
SELECT TAHUN_BUKU,BULAN,UPTB,SUM(TARGET) TARGET 
FROM VW_TARGET_BULAN_UPTB_6
WHERE PAJAK_ID=9
GROUP BY TAHUN_BUKU,BULAN,UPTB
) A
LEFT JOIN (
SELECT TAHUN_BUKU,UPTB,1 BULAN,
            SUM(DECODE(JENIS_BUKU,0,1,0)) SPPT_MURNI,
            SUM(DECODE(JENIS_BUKU,0,KETETAPAN,0)) KETETAPAN_MURNI,
            SUM(DECODE(JENIS_BUKU,1,1,0)) SPPT_TUNGGAKAN,
            SUM(DECODE(JENIS_BUKU,1,KETETAPAN,0)) KETETAPAN_TUNGGAKAN
FROM MV_DBE_MON_PBB
GROUP BY TAHUN_BUKU,UPTB
) B ON A.TAHUN_BUKU=B.TAHUN_BUKU AND A.BULAN=B.BULAN AND A.UPTB=B.UPTB
LEFT JOIN
(
SELECT TAHUN_BUKU,UPTB,EXTRACT (MONTH FROM TGL_BAYAR) BULAN_BAYAR,
           SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=0 THEN REALISASI
                    ELSE
                    0 END
                 ) REALISASI_MURNI,
            SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=1 THEN REALISASI
                    ELSE
                    0 END
                 ) REALISASI_TUNGGAKAN,     
              SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=0 THEN 1
                    ELSE
                    0 END
                 ) REALISASI_SPPT_MURNI,
            SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=1 THEN 1
                    ELSE
                    0 END
                 ) REALISASI_SPPT_TUNGGAKAN     
FROM MV_DBE_MON_PBB A
WHERE TAHUN_BUKU_BAYAR IS NOT NULL 
GROUP BY TAHUN_BUKU,UPTB,EXTRACT (MONTH FROM TGL_BAYAR)
) C ON A.TAHUN_BUKU=C.TAHUN_BUKU AND A.BULAN=C.BULAN_BAYAR AND A.UPTB=C.UPTB</Sql>
      </Query>
      <Query Type="CustomSqlQuery" Name="KategoriNama">
        <Sql>SELECT A.*,TARGET
FROM (
SELECT A.*,REALISASI_MURNI,REALISASI_TUNGGAKAN,REALISASI_SPPT_MURNI,REALISASI_SPPT_TUNGGAKAN
FROM (
SELECT TAHUN_BUKU,KATEGORI_NAMA,
            SUM(DECODE(JENIS_BUKU,0,1,0)) SPPT_MURNI,
            SUM(DECODE(JENIS_BUKU,0,KETETAPAN,0)) KETETAPAN_MURNI,
            SUM(DECODE(JENIS_BUKU,1,1,0)) SPPT_TUNGGAKAN,
            SUM(DECODE(JENIS_BUKU,1,KETETAPAN,0)) KETETAPAN_TUNGGAKAN
FROM MV_DBE_MON_PBB
GROUP BY TAHUN_BUKU,KATEGORI_NAMA
) A
LEFT JOIN (
SELECT TAHUN_BUKU,KATEGORI_NAMA,
           SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=0 THEN REALISASI
                    ELSE
                    0 END
                 ) REALISASI_MURNI,
            SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=1 THEN REALISASI
                    ELSE
                    0 END
                 ) REALISASI_TUNGGAKAN,     
              SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=0 THEN 1
                    ELSE
                    0 END
                 ) REALISASI_SPPT_MURNI,
            SUM(
                    CASE
                        WHEN TAHUN_BUKU_BAYAR=TAHUN_BUKU AND JENIS_BUKU=1 THEN 1
                    ELSE
                    0 END
                 ) REALISASI_SPPT_TUNGGAKAN     
FROM MV_DBE_MON_PBB A
WHERE TAHUN_BUKU_BAYAR IS NOT NULL 
GROUP BY TAHUN_BUKU,KATEGORI_NAMA
) B ON A.TAHUN_BUKU=B.TAHUN_BUKU AND A.KATEGORI_NAMA=B.KATEGORI_NAMA
) A
LEFT JOIN 
(
SELECT TAHUN_BUKU,SUM(TARGET) TARGET 
FROM VW_TARGET_BULAN_UPTB_6
WHERE PAJAK_ID=9
GROUP BY TAHUN_BUKU
) B ON A.TAHUN_BUKU=B.TAHUN_BUKU</Sql>
